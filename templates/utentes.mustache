<div class="container">
  <table class="table table-striped table-dark table-hover">
    <thead>
      <tr>
        <th scope="col">Nome</th>
        <th scope="col">Cidade</th>
      </tr>
    </thead>
    <tbody>
      {{#utentes}}
      <tr>
        <td>{{ nome }}</td>
        <td>{{ cidade }}</td>
      </tr>
      {{/utentes}}
    </tbody>
  </table>
  <div class="paginacao">
    {{&pagination}}
  </div>
</div>
