<!--     ENFERMEIROS       -->
<!--                       -->
<!--------------------------->

<div id="enfermeiros-list"></div>
<button
  type="button"
  class="btn btn-warning"
  data-toggle="modal"
  data-target="#enfermeiroModal"
>
  Adicionar enfermeiro
</button>
<!-- Begin enfermeiro modal -->
<div
  class="modal fade"
  id="enfermeiroModal"
  tabindex="-1"
  role="dialog"
  aria-labelledby="enfermeiroModal"
  aria-hidden="true"
>
  <div class="modal-dialog modal-wider" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="enfermeiroModalLabel">
          Selecione um enfermeiro a adicionar
        </h5>
        <button
          type="button"
          class="close"
          data-dismiss="modal"
          aria-label="Cancelar"
        >
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <div class="input-group mb-3">
          <input
            id="enfermeiro-search"
            type="text"
            class="form-control"
            placeholder="Pesquisar..."
            aria-label="Pesquisar..."
            aria-describedby="basic-addon2"
          />
          <div class="input-group-append">
            <button
              id="enfermeiro-search-submit"
              class="btn btn-outline-secondary"
              type="button"
            >
              Pesquisar
            </button>
          </div>
          <table class="table table-striped table-hover">
            <thead>
              <tr>
                <th scope="col">Nome</th>
                <th scope="col">Especialidade</th>
                <th scope="col">Cidade</th>
              </tr>
            </thead>
            <tbody id="enfermeiro-results">
            </tbody>
          </table>
        </div>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">
          Cancelar
        </button>
        <button
          id="insert-btn"
          type="submit"
          class="btn btn-success"
          role="button"
        >
          Agendar
        </button>
      </div>
    </div>
  </div>
</div>
<!-- End enfermeiro modal -->

